{
  "name": "graphql-example",
  "version": "1.0.0",
  "description": "An example on using a GraphQL endpoint to store and get data to/from DynamoDB",
  "files": [
    "bin"
  ],
  "scripts": {
    "create": "npm run transpile && claudia create --name claudia-graphql-example --region us-east-1 --api-module bin/index --policies policies",
    "deploy": "npm run transpile && claudia update",
    "transpile": "babel --presets es2015 --plugins add-module-exports src --out-dir bin"
  },
  "keywords": [
    "claudia",
    "aws-lambda",
    "DynamoDB",
    "GraphQL"
  ],
  "author": "Fancis Au-Yeung @aidanbon",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/claudiajs/example-projects.git"
  },
  "dependencies": {
    "aws-sdk": "^2.7.27",
    "bluebird": "^3.3.3",
    "claudia-api-builder": "^2.4.0",
    "graphql": "0.4.18"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.6.5",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.6.0",
    "claudia": "^2.6.0"
  }
}
